{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "wfs",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "window_declaration"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "window_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "window"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "window_attribute"
          }
        },
        {
          "type": "SYMBOL",
          "name": "fields_block"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "window_attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "stream_attribute"
        },
        {
          "type": "SYMBOL",
          "name": "time_attribute"
        },
        {
          "type": "SYMBOL",
          "name": "over_attribute"
        }
      ]
    },
    "stream_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "stream"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string"
            },
            {
              "type": "SYMBOL",
              "name": "string_array"
            }
          ]
        }
      ]
    },
    "string_array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "string"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "time_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "time"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "over_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "over"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "STRING",
              "value": "0"
            }
          ]
        }
      ]
    },
    "fields_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "fields"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "field_declaration"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "field_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "field_name"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "field_type"
          }
        }
      ]
    },
    "field_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dotted_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "dotted_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "quoted_identifier": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "`"
          },
          {
            "type": "PATTERN",
            "value": "[^`]+"
          },
          {
            "type": "STRING",
            "value": "`"
          }
        ]
      }
    },
    "field_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "array_type"
        },
        {
          "type": "SYMBOL",
          "name": "base_type"
        }
      ]
    },
    "array_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "array"
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "SYMBOL",
          "name": "base_type"
        }
      ]
    },
    "base_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "chars"
        },
        {
          "type": "STRING",
          "value": "digit"
        },
        {
          "type": "STRING",
          "value": "float"
        },
        {
          "type": "STRING",
          "value": "bool"
        },
        {
          "type": "STRING",
          "value": "time"
        },
        {
          "type": "STRING",
          "value": "ip"
        },
        {
          "type": "STRING",
          "value": "hex"
        }
      ]
    },
    "duration": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+[smhd]"
      }
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\"\\\\]"
                },
                {
                  "type": "PATTERN",
                  "value": "\\\\."
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}